    <!-- Main hero unit for a primary marketing message or call to action -->
    <div class="hero-unit">
      <h1>Massive Bear!</h1>
      <p>This is a seed project for using angular, boostrap, grunt, express and static mustache. Click below to move from this static page to a single page app called myApp.</p>
      <p><a href="/myApp" class="btn btn-primary btn-large">View myApp &raquo;</a></p>
    </div>

   <!-- Example row of columns -->
